{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Debug by ts-node",
          "type": "node",
          "request": "launch",
          "args": [
            "${workspaceRoot}/src/index"
          ],
          "runtimeArgs": [
            "--nolazy",
            "-r",
            "ts-node/register"
          ],
          "sourceMaps": true,
          "cwd": "${workspaceRoot}",
          "protocol": "inspector",
          "console": "integratedTerminal",
          "internalConsoleOptions": "neverOpen"
      },
  ]
}
